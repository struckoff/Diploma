{
  "devDependencies": {
    "browserify": "latest",
    "watchify": "latest"
  },
  "scripts": {
    "build-js": "browserify -t reactify js/create_test_ui/create_test.js > js/create_test_ui/create_test_raw.js",
    "watch-create_test": "watchify -t reactify js/create_test_ui/create_test.js -o js/create_test_ui/create_test_raw.js",
    "watch-edit_test": "watchify -t reactify js/create_test_ui/edit_test.js -o js/create_test_ui/edit_test_raw.js",
    "watch-modules": "watchify -t reactify js/create_test_ui/modules.js -o js/create_test_ui/modules_raw.js",
    "watch-test": "watchify -t reactify js/test_ui/test.js -o js/test_ui/test_raw.js",
    "refresh-create_test": "refreshify -t reactify js/create_test_ui/create_test.js -o js/create_test_ui/create_test_raw.js",
    "refresh-edit_test": "refreshify -t reactify js/create_test_ui/edit_test.js -o js/create_test_ui/edit_test_raw.js",
    "refresh-modules": "refreshify -t reactify js/create_test_ui/modules.js -o js/create_test_ui/modules_raw.js",
    "refresh-test": "refreshify -t reactify js/test_ui/test.js -o js/test_ui/test_raw.js"
  },
  "dependencies": {
    "jquery": "^2.2.3",
    "npm-run-all": "^1.5.1",
    "react-codemirror": "^0.2.6",
    "react-modal": "^0.6.1"
  }
}
